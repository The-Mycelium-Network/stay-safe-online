{
  "name": "stay-safe-online",
  "version": "1.0.0",
  "description": "Resources that helps everyone stay safe online",
  "main": "./src/index.html",
  "scripts": {
    "dev:parcel": "parcel src/index.html",
    "prod:parcel": "parcel build src/index.html --out-dir docs/ --public-url \"/stay-safe-online/\"",
    "post-process": "node ./lib/simple-postprocessor",
    "format": "prettier --write \"src/**/*.{js,css,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,css,json}\"",
    "lint": "eslint \"src/**/*.js\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThirdWorldProblems/stay-safe-online.git"
  },
  "keywords": [
    "civic-tech",
    "privacy",
    "security",
    "resources"
  ],
  "author": "ThirdWorldProblems",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThirdWorldProblems/stay-safe-online/issues"
  },
  "homepage": "https://github.com/ThirdWorldProblems/stay-safe-online#readme",
  "devDependencies": {
    "eslint": "8.19.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "fs-extra": "7.0.1",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.5",
    "prettier": "2.7.1"
  }
}
